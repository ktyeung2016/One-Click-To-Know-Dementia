<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:tools="http://schemas.android.com/tools"
	android:id="@+id/holepage"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical" >
	<RelativeLayout
		android:layout_width="wrap_content"
		android:layout_height="0dp"
		android:layout_weight="2" >
		<ImageView
			android:id="@+id/qweqeqe"
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:layout_alignParentBottom="true"
			android:scaleType="fitXY"
			android:src="@drawable/actionbar" />
		<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:layout_alignParentRight="true" >
			<ImageButton
				android:id="@+id/back"
				android:layout_width="wrap_content"
				android:layout_height="match_parent"
				android:layout_weight="1"
				android:background="@null"
				android:src="@null"
                android:contentDescription="@string/backButton"/>
			<TextView
				android:id="@+id/title"
				android:layout_width="wrap_content"
				android:layout_height="match_parent"
				android:layout_weight="3"
				android:gravity="center"
				android:text="@string/socialres"
				android:textColor="#FFFFFF"
				android:textSize="?attr/font_small"
				android:textStyle="bold" />
			<ImageButton
				android:id="@+id/menu"
				android:layout_width="wrap_content"
				android:layout_height="match_parent"
				android:layout_weight="1"
				android:background="@null"
				android:src="@null"
                android:contentDescription="@string/menuButton"/>
		</LinearLayout>
	</RelativeLayout>
	<LinearLayout
		android:layout_width="match_parent"
		android:layout_height="0dp"
		android:layout_weight="3" >
		<TextView
			android:id="@+id/textView1"
			android:layout_width="0dp"
			android:layout_height="match_parent"
			android:layout_weight="1"
			android:gravity="center"
			android:text="@string/area"
			android:textSize="?attr/font_small"
            android:contentDescription="@string/area"
            android:labelFor="@+id/area"
            android:focusable="true"/>
		<TextView
			android:id="@+id/area"
			android:layout_width="0dp"
			android:layout_height="38dp"
			android:layout_weight="1.49"
			android:background="@drawable/btn_socres_spinner"
			android:gravity="center"
			android:paddingRight="20dp"
			android:text="全部"
			android:textColor="#ffffff"
			android:textSize="?attr/font_small"
            android:contentDescription="@string/areaOption"/>
		<TextView
			android:layout_width="0dp"
			android:layout_height="match_parent"
			android:layout_weight="1"
			android:gravity="center"
			android:text="@string/type"
            android:contentDescription="@string/type"
			android:textSize="?attr/font_small"
            android:labelFor="@+id/type"
            android:focusable="true" />
		<TextView
			android:id="@+id/type"
			android:layout_width="0dp"
			android:layout_height="38dp"
			android:layout_weight="1.5"
			android:background="@drawable/btn_socres_spinner"
			android:gravity="center"
			android:paddingRight="20dp"
			android:text="全部"
			android:textColor="#ffffff"
			android:textSize="?attr/font_small"
            android:contentDescription="@string/typeOption" />
	</LinearLayout>
	<LinearLayout
		android:layout_width="match_parent"
		android:layout_height="0dp"
		android:layout_weight="17"
		android:orientation="vertical" >
		<ExpandableListView
			android:id="@+id/lvExp"
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:groupIndicator="@null" >
		</ExpandableListView>
	</LinearLayout>
</LinearLayout><!-- From: file:/D:/proj/jccpa/source_code/AndroidStudio/Dementia/dementia/src/main/res/layout/activity_socres.xml -->