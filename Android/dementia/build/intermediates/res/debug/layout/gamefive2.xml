                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="1.00"
                                android:gravity="center"
                                android:text="$4"
                                android:textSize="?attr/font_medium" />

                            <TextView
                                android:id="@+id/textView6"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="1.00"
                                android:gravity="center"
                                android:text="3"
                                android:textSize="?attr/font_medium" />
                        </LinearLayout>
                    </LinearLayout>

