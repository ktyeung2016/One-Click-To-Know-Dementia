                            android:layout_height="match_parent"
                            android:layout_weight="3"
                            android:paddingBottom="5dp"
                            android:paddingRight="20dp"
                            android:paddingTop="5dp">

                            <TextView
                                android:id="@+id/textView7"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="3"
