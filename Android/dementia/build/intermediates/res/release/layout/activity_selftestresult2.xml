<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:tools="http://schemas.android.com/tools"
	android:id="@+id/holepage"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical" >
	<RelativeLayout
		android:layout_width="wrap_content"
		android:layout_height="0dp"
		android:layout_weight="2" >
		<ImageView
			android:id="@+id/qweqeqe"
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:layout_alignParentBottom="true"
			android:scaleType="fitXY"
			android:src="@drawable/actionbar" />
		<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:layout_alignParentRight="true" >
			<ImageButton
				android:id="@+id/back"
				android:layout_width="wrap_content"
				android:layout_height="match_parent"
				android:layout_weight="1"
				android:background="@null"
				android:src="@null"
                android:contentDescription="@string/backButton"/>
			<TextView
				android:id="@+id/title"
				android:layout_width="wrap_content"
				android:layout_height="match_parent"
				android:layout_weight="3"
				android:gravity="center"
				android:text="@string/selfas"
				android:textColor="#FFFFFF"
				android:textSize="?attr/font_small"
				android:textStyle="bold" />
			<ImageButton
				android:id="@+id/menu"
				android:layout_width="wrap_content"
				android:layout_height="match_parent"
				android:layout_weight="1"
				android:background="@null"
				android:src="@null"
                android:contentDescription="@string/menuButton"/>
		</LinearLayout>
	</RelativeLayout>
	<LinearLayout
		android:layout_width="match_parent"
		android:layout_height="0dp"
		android:layout_weight="20"
		android:orientation="vertical" >
		<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="0dp"
			android:layout_weight="2"
			android:orientation="vertical" >
			<TextView
				android:id="@+id/textView1"
				android:layout_width="match_parent"
				android:layout_height="match_parent"
				android:gravity="center"
				android:text="@string/tenfeatures"
				android:textColor="#03736B"
				android:textSize="?attr/font_xlarge"
				android:textStyle="bold" />
		</LinearLayout>
		<TextView
			android:id="@+id/textView2"
			android:layout_width="match_parent"
			android:layout_height="0dp"
			android:layout_weight="1"
			android:gravity="center"
			android:text="@string/selfresult"
			android:textColor="#A13155"
			android:textSize="?attr/font_xxlarge"
			android:textStyle="bold" />
		<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="0dp"
			android:layout_weight="15"
			android:background="#ffffff"
			android:orientation="vertical"
			android:paddingLeft="10dp"
			android:paddingRight="10dp" >
			<LinearLayout
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:background="@drawable/selftestresultboard"
				android:orientation="vertical" >
				<LinearLayout
					android:layout_width="match_parent"
					android:layout_height="0dp"
					android:layout_weight="1" >
					<TextView
						android:id="@+id/textView3"
						android:layout_width="0dp"
						android:layout_height="match_parent"
						android:layout_weight="1"
						android:gravity="top|right"
						android:text="@string/marks"
						android:textColor="#372707"
						android:textSize="?attr/font_xlarge"
						android:textStyle="bold" />
					<TextView
						android:id="@+id/textView3"
						android:layout_width="0dp"
						android:layout_height="match_parent"
						android:layout_weight="1"
						android:gravity="top|left"
						android:text=":74分"
						android:textColor="#02726B"
						android:textSize="?attr/font_xlarge"
						android:textStyle="bold" />
				</LinearLayout>
			</LinearLayout>
			<TextView
				android:id="@+id/textView4"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:gravity="center"
				android:text="根據評分，你家長者在過往五年間的認知能力有顯著的退減。"
				android:textSize="?attr/font_medium" />
			<LinearLayout
				android:layout_width="match_parent"
				android:layout_height="50dp" >
				<LinearLayout
					android:layout_width="0dp"
					android:layout_height="match_parent"
					android:layout_weight="1" >
				</LinearLayout>
				<TextView
					android:id="@+id/ans1"
					android:layout_width="0dp"
					android:layout_height="wrap_content"
					android:layout_weight="11"
					android:background="@drawable/btn_yellow"
					android:gravity="center"
					android:text="@string/relainfo"
					android:textSize="?attr/font_medium"
					android:textStyle="bold" />
				<LinearLayout
					android:layout_width="0dp"
					android:layout_height="match_parent"
					android:layout_weight="1" >
				</LinearLayout>
			</LinearLayout>
		</LinearLayout>
		<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="0dp"
			android:layout_weight="2.5"
			android:orientation="vertical" >
			<LinearLayout
				android:layout_width="match_parent"
				android:layout_height="0dp"
				android:layout_weight="1"
				android:background="#BFDA44"
				android:paddingTop="10dp" >
				<LinearLayout
					android:layout_width="0dp"
					android:layout_height="match_parent"
					android:layout_weight="5" >
				</LinearLayout>
				<Button
					android:id="@+id/playag"
					android:layout_width="0dp"
					android:layout_height="46dp"
					android:layout_weight="4.02"
					android:background="@drawable/btn_refresh"
					android:paddingLeft="20dp"
					android:text="@string/playag"
					android:textColor="#ffffff" />
				<LinearLayout
					android:layout_width="0dp"
					android:layout_height="match_parent"
					android:layout_weight="5" >
				</LinearLayout>
			</LinearLayout>
		</LinearLayout>
	</LinearLayout>
</LinearLayout><!-- From: file:/D:/proj/jccpa/source_code/AndroidStudio/Dementia/dementia/src/main/res/layout/activity_selftestresult2.xml -->