<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:tools="http://schemas.android.com/tools"
	android:id="@+id/holepage"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical" >
	<RelativeLayout
		android:layout_width="wrap_content"
		android:layout_height="0dp"
		android:layout_weight="2" >
		<ImageView
			android:id="@+id/qweqeqe"
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:layout_alignParentBottom="true"
			android:scaleType="fitXY"
			android:src="@drawable/actionbar" />
		<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:layout_alignParentRight="true" >
			<ImageButton
				android:id="@+id/back"
				android:layout_width="wrap_content"
				android:layout_height="match_parent"
				android:layout_weight="1"
				android:background="@null"
				android:src="@null"
                android:contentDescription="@string/backButton"/>
			<TextView
				android:id="@+id/title"
				android:layout_width="wrap_content"
				android:layout_height="match_parent"
				android:layout_weight="3"
				android:gravity="center"
				android:text="@string/mindcal"
				android:textColor="#FFFFFF"
				android:textSize="?attr/font_small"
				android:textStyle="bold" />
			<ImageButton
				android:id="@+id/menu"
				android:layout_width="wrap_content"
				android:layout_height="match_parent"
				android:layout_weight="1"
				android:background="@null"
				android:src="@null"
                android:contentDescription="@string/menuButton"/>
		</LinearLayout>
	</RelativeLayout>
	<LinearLayout
		android:layout_width="match_parent"
		android:layout_height="0dp"
		android:layout_weight="20"
		android:orientation="vertical" >
		<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="0dp"
			android:layout_weight="19"
			android:orientation="vertical" >
			<LinearLayout
				android:layout_width="match_parent"
				android:layout_height="0dp"
				android:layout_weight="0.5"
				android:orientation="vertical"
				android:paddingLeft="5dp" >
				<TextView
					android:id="@+id/qnum2"
					android:layout_width="60dp"
					android:layout_height="match_parent"
					android:background="@drawable/game_numberofq"
					android:gravity="center"
					android:text="20"
					android:textColor="#ffffff" />
			</LinearLayout>
			<LinearLayout
				android:layout_width="match_parent"
				android:layout_height="0dp"
				android:layout_weight="1.5"
				android:orientation="vertical" >
				<TextView
					android:id="@+id/textView3"
					android:layout_width="match_parent"
					android:layout_height="match_parent"
					android:gravity="center"
					android:text="@string/caltotal"
					android:textSize="?attr/font_xxlarge" />
			</LinearLayout>
			<LinearLayout
				android:layout_width="match_parent"
				android:layout_height="0dp"
				android:layout_weight="5.5"
				android:background="@drawable/game_cal_bg"
				android:orientation="vertical" >
				<LinearLayout
					android:layout_width="match_parent"
					android:layout_height="0dp"
					android:layout_weight="1"
					android:paddingTop="5dp" >
					<TextView
						android:id="@+id/textView4"
						android:layout_width="0dp"
						android:layout_height="wrap_content"
						android:layout_weight="1"
						android:gravity="center"
						android:text="@string/item"
						android:textSize="30dp" />
					<TextView
						android:id="@+id/textView2"
						android:layout_width="0dp"
						android:layout_height="wrap_content"
						android:layout_weight="1"
						android:gravity="center"
						android:text="@string/price"
						android:textSize="30dp" />
					<TextView
						android:id="@+id/textView5"
						android:layout_width="0dp"
						android:layout_height="wrap_content"
						android:layout_weight="1"
						android:gravity="center"
						android:text="@string/quatity"
						android:textSize="30dp" />
				</LinearLayout>
				<LinearLayout
					android:id="@+id/setone"
					android:layout_width="match_parent"
					android:layout_height="0dp"
					android:layout_weight="1"
					android:paddingTop="5dp" >
					<RelativeLayout
						android:layout_width="0dp"
						android:layout_height="match_parent"
						android:layout_weight="1" >
						<ImageView
							android:id="@+id/setoneimg"
							android:layout_width="match_parent"
							android:layout_height="match_parent"
							android:src="@drawable/orange_test_01" />
						<ImageView
							android:id="@+id/imageView1"
							android:layout_width="match_parent"
							android:layout_height="match_parent"
							android:layout_centerHorizontal="true"
							android:layout_centerVertical="true"
							android:src="@drawable/outline_item" />
					</RelativeLayout>
					<TextView
						android:id="@+id/setoneprice"
						android:layout_width="0dp"
						android:layout_height="match_parent"
						android:layout_weight="1.00"
						android:gravity="center"
						android:text="$4"
						android:textSize="?attr/font_medium" />
					<TextView
						android:id="@+id/setoneq"
						android:layout_width="0dp"
						android:layout_height="match_parent"
						android:layout_weight="1.00"
						android:gravity="center"
						android:text="3"
						android:textSize="?attr/font_medium" />
				</LinearLayout>
				<LinearLayout
					android:id="@+id/settwo"
					android:layout_width="match_parent"
					android:layout_height="0dp"
					android:layout_weight="1"
					android:paddingTop="5dp" >
					<RelativeLayout
						android:layout_width="0dp"
						android:layout_height="match_parent"
						android:layout_weight="1" >
						<ImageView
							android:id="@+id/settwoimg"
							android:layout_width="match_parent"
							android:layout_height="match_parent"
							android:src="@drawable/orange_test_01" />
						<ImageView
							android:id="@+id/imageView1"
							android:layout_width="match_parent"
							android:layout_height="match_parent"
							android:layout_centerHorizontal="true"
							android:layout_centerVertical="true"
							android:src="@drawable/outline_item" />
					</RelativeLayout>
					<TextView
						android:id="@+id/settwoprice"
						android:layout_width="0dp"
						android:layout_height="match_parent"
						android:layout_weight="1.00"
						android:gravity="center"
						android:text="$4"
						android:textSize="?attr/font_medium" />
					<TextView
						android:id="@+id/settwoq"
						android:layout_width="0dp"
						android:layout_height="match_parent"
						android:layout_weight="1.00"
						android:gravity="center"
						android:text="3"
						android:textSize="?attr/font_medium" />
				</LinearLayout>
				<LinearLayout
					android:id="@+id/setthree"
					android:layout_width="match_parent"
					android:layout_height="0dp"
					android:layout_weight="1"
					android:paddingTop="5dp" >
					<RelativeLayout
						android:layout_width="0dp"
						android:layout_height="match_parent"
						android:layout_weight="1" >
						<ImageView
							android:id="@+id/setthreeimg"
							android:layout_width="match_parent"
							android:layout_height="match_parent"
							android:src="@drawable/apple_test_01" />
						<ImageView
							android:id="@+id/outline"
							android:layout_width="match_parent"
							android:layout_height="match_parent"
							android:layout_alignParentLeft="true"
							android:layout_alignParentTop="true"
							android:src="@drawable/outline_item" />
					</RelativeLayout>
					<TextView
						android:id="@+id/setthreeprice"
						android:layout_width="0dp"
						android:layout_height="match_parent"
						android:layout_weight="1.00"
						android:gravity="center"
						android:text="$4"
						android:textSize="?attr/font_medium" />
					<TextView
						android:id="@+id/setthreeq"
						android:layout_width="0dp"
						android:layout_height="match_parent"
						android:layout_weight="1.00"
						android:gravity="center"
						android:text="3"
						android:textSize="?attr/font_medium" />
				</LinearLayout>
				<LinearLayout
					android:layout_width="match_parent"
					android:layout_height="0dp"
					android:layout_weight="1"
					android:paddingTop="5dp" >
					<LinearLayout
						android:layout_width="0dp"
						android:layout_height="match_parent"
						android:layout_weight="3"
						android:paddingBottom="5dp"
						android:paddingRight="20dp"
						android:paddingTop="5dp" >
						<TextView
							android:id="@+id/textView7"
							android:layout_width="0dp"
							android:layout_height="match_parent"
							android:layout_weight="3"
							android:gravity="center"
							android:text="@string/totalprice"
							android:textSize="?attr/font_medium" />
						<EditText
							android:id="@+id/totalprice"
							android:layout_width="0dp"
							android:layout_height="match_parent"
							android:layout_weight="2"
							android:background="@drawable/bg_game_cal_input"
							android:inputType="numberDecimal" >
							<requestFocus />
						</EditText>
					</LinearLayout>
					<LinearLayout
						android:layout_width="0dp"
						android:layout_height="match_parent"
						android:layout_weight="2"
						android:paddingBottom="5dp"
						android:paddingLeft="5dp"
						android:paddingRight="5dp"
						android:paddingTop="5dp" >
						<Button
							android:id="@+id/enter"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:background="@drawable/btn"
							android:text="@string/enter"
							android:textColor="#ffffff" />
					</LinearLayout>
				</LinearLayout>
			</LinearLayout>
			<LinearLayout
				android:layout_width="match_parent"
				android:layout_height="0dp"
				android:layout_weight="1"
				android:background="#BFDA44"
				android:paddingTop="10dp" >
				<LinearLayout
					android:layout_width="0dp"
					android:layout_height="match_parent"
					android:layout_weight="1" >
					<ImageView
						android:id="@+id/imageView4"
						android:layout_width="match_parent"
						android:layout_height="match_parent"
						android:scaleType="centerInside"
						android:src="@drawable/icon_notice" />
				</LinearLayout>
				<LinearLayout
					android:layout_width="0dp"
					android:layout_height="match_parent"
					android:layout_weight="3.98" >
					<ImageView
						android:id="@+id/h5"
						android:layout_width="0dp"
						android:layout_height="match_parent"
						android:layout_weight="1"
						android:scaleType="centerInside"
						android:src="@drawable/icon_heart" />
					<ImageView
						android:id="@+id/h4"
						android:layout_width="0dp"
						android:layout_height="match_parent"
						android:layout_weight="1"
						android:scaleType="centerInside"
						android:src="@drawable/icon_heart" />
					<ImageView
						android:id="@+id/h3"
						android:layout_width="0dp"
						android:layout_height="match_parent"
						android:layout_weight="1"
						android:scaleType="centerInside"
						android:src="@drawable/icon_heart" />
					<ImageView
						android:id="@+id/h2"
						android:layout_width="0dp"
						android:layout_height="match_parent"
						android:layout_weight="1"
						android:scaleType="centerInside"
						android:src="@drawable/icon_heart" />
					<ImageView
						android:id="@+id/h1"
						android:layout_width="0dp"
						android:layout_height="match_parent"
						android:layout_weight="1"
						android:scaleType="centerInside"
						android:src="@drawable/icon_heart" />
				</LinearLayout>
				<LinearLayout
					android:layout_width="0dp"
					android:layout_height="match_parent"
					android:layout_weight="1" >
					<ImageView
						android:id="@+id/demo"
						android:layout_width="match_parent"
						android:layout_height="match_parent"
						android:scaleType="centerInside"
						android:src="@drawable/icon_dome" />
				</LinearLayout>
				<LinearLayout
					android:layout_width="wrap_content"
					android:layout_height="match_parent" >
				</LinearLayout>
			</LinearLayout>
		</LinearLayout>
	</LinearLayout>
</LinearLayout>