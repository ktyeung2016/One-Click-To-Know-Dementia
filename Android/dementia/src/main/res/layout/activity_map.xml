<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:tools="http://schemas.android.com/tools"
	android:id="@+id/holepage"
	android:layout_width="match_parent"
	android:layout_height="match_parent" >
	<LinearLayout
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:orientation="vertical" >
		<RelativeLayout
			android:layout_width="wrap_content"
			android:layout_height="0dp"
			android:layout_weight="2" >
			<ImageView
				android:id="@+id/qweqeqe"
				android:layout_width="match_parent"
				android:layout_height="match_parent"
				android:layout_alignParentBottom="true"
				android:scaleType="fitXY"
				android:src="@drawable/actionbar" />
			<LinearLayout
				android:layout_width="match_parent"
				android:layout_height="match_parent"
				android:layout_alignParentRight="true" >
				<ImageButton
					android:id="@+id/back"
					android:layout_width="wrap_content"
					android:layout_height="match_parent"
					android:layout_weight="1"
					android:background="@null"
					android:src="@null"
                    android:contentDescription="@string/backButton"/>
				<TextView
					android:id="@+id/title"
					android:layout_width="wrap_content"
					android:layout_height="match_parent"
					android:layout_weight="3"
					android:gravity="center"
					android:text="TextView"
					android:textColor="#FFFFFF"
					android:textSize="?attr/font_small"
					android:textStyle="bold" />
				<ImageButton
					android:id="@+id/menu"
					android:layout_width="wrap_content"
					android:layout_height="match_parent"
					android:layout_weight="1"
					android:background="@null"
					android:src="@null"
                    android:contentDescription="@string/menuButton"/>
			</LinearLayout>
		</RelativeLayout>
		<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="0dp"
			android:layout_weight="20"
			android:orientation="vertical" >
			<RelativeLayout
				android:layout_width="match_parent"
				android:layout_height="0dp"
				android:layout_weight="7" >
				<com.google.android.gms.maps.MapView
					android:id="@+id/map"
					android:layout_width="match_parent"
					android:layout_height="match_parent" />
			</RelativeLayout>
			<LinearLayout
				android:id="@+id/buttonlayout"
				android:layout_width="match_parent"
				android:layout_height="0dp"
				android:layout_weight="1"
				android:background="#BFDA44"
				android:paddingTop="10dp" >
				<LinearLayout
					android:layout_width="0dp"
					android:layout_height="match_parent"
					android:layout_weight="1" >
				</LinearLayout>
				<Button
					android:id="@+id/resetlocation"
					android:layout_width="0dp"
					android:layout_height="46dp"
					android:layout_weight="3.98"
					android:background="@drawable/btnresetlopcation"
					android:paddingLeft="20dp"
					android:text="@string/resetlocation"
					android:textColor="#ffffff" />
				<LinearLayout
					android:layout_width="0dp"
					android:layout_height="match_parent"
					android:layout_weight="1" >
				</LinearLayout>
				<Button
					android:id="@+id/callfmy"
					android:layout_width="0dp"
					android:layout_height="46dp"
					android:layout_weight="4.02"
					android:background="@drawable/btncallfmyred"
					android:paddingLeft="20dp"
					android:text="@string/callfmy"
					android:textColor="#ffffff" />
				<LinearLayout
					android:layout_width="0dp"
					android:layout_height="match_parent"
					android:layout_weight="1" >
				</LinearLayout>
			</LinearLayout>
		</LinearLayout>
	</LinearLayout>
	<RelativeLayout
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:visibility="gone" >
		<LinearLayout
			android:id="@+id/wholelayout"
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:orientation="vertical" >
			<LinearLayout
				android:layout_width="match_parent"
				android:layout_height="0dp"
				android:layout_weight="1.9"
				android:orientation="vertical" >
			</LinearLayout>
			<LinearLayout
				android:id="@+id/topbarlayout"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:background="#ffffff"
				android:gravity="center"
				android:paddingLeft="10dp" >
				<TextView
					android:id="@+id/textView1"
					android:layout_width="0dp"
					android:layout_height="wrap_content"
					android:layout_weight="3"
					android:text="@string/mapfunctionstep"
					android:textSize="?attr/font_medium" />
				<ImageView
					android:id="@+id/imageView1"
					android:layout_width="0dp"
					android:layout_height="wrap_content"
					android:layout_weight="1"
					android:scaleType="centerInside"
					android:src="@drawable/btn_back" />
				<TextView
					android:id="@+id/textView2"
					android:layout_width="0dp"
					android:layout_height="wrap_content"
					android:layout_weight="1"
					android:gravity="center"
					android:text="1/12"
					android:textSize="?attr/font_small" />
				<ImageView
					android:id="@+id/imageView2"
					android:layout_width="0dp"
					android:layout_height="wrap_content"
					android:layout_weight="1"
					android:scaleType="centerInside"
					android:src="@drawable/btn_next" />
			</LinearLayout>
			<RelativeLayout
				android:id="@+id/demolayout"
				android:layout_width="match_parent"
				android:layout_height="0dp"
				android:layout_weight="15"
				android:background="@drawable/transparentbg"
				android:orientation="vertical" >
			</RelativeLayout>
			<LinearLayout
				android:layout_width="match_parent"
				android:layout_height="0dp"
				android:layout_weight="2.4"
				android:orientation="vertical" >
			</LinearLayout>
		</LinearLayout>
	</RelativeLayout>
</RelativeLayout>