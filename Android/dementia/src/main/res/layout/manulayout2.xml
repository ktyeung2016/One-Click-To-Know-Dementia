<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="310dp"
	android:layout_height="match_parent"
	android:background="@drawable/menu2"
	android:orientation="vertical" >
	<RelativeLayout
		android:layout_width="match_parent"
		android:layout_height="0dp"
		android:layout_weight="1" >
		<ImageView
			android:id="@+id/imageView1"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_alignParentLeft="true"
			android:scaleType="fitXY"
			android:src="@drawable/menu1" />
		<ImageView
			android:id="@+id/imageView2"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_centerHorizontal="true"
			android:layout_centerVertical="true"
			android:src="@drawable/jclogo" />
	</RelativeLayout>
	<LinearLayout
		android:layout_width="match_parent"
		android:layout_height="0dp"
		android:layout_weight="10"
		android:orientation="vertical"
		android:paddingLeft="10dp"
		android:paddingRight="10dp"
		android:paddingTop="5dp" >
		<RelativeLayout
			android:layout_width="match_parent"
			android:layout_height="0dp"
			android:layout_weight="1"
			android:gravity="center"
			android:orientation="vertical"
			android:paddingLeft="10dp"
			android:paddingTop="1dp" >
			<ImageView
				android:id="@+id/mylocation"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:src="@drawable/menumylocation"
                android:contentDescription="@string/mylocation" />
		</RelativeLayout>
		<RelativeLayout
			android:layout_width="match_parent"
			android:layout_height="0dp"
			android:layout_weight="1"
			android:gravity="center"
			android:orientation="vertical" >
			<TextView
				android:id="@+id/home"
				android:layout_width="match_parent"
				android:layout_height="match_parent"
				android:layout_alignParentBottom="true"
				android:layout_alignParentRight="true"
				android:layout_alignParentTop="true"
				android:background="@drawable/menuhome1"
				android:gravity="center"
				android:paddingLeft="40dp"
				android:text="@string/homepage"
				android:textColor="#ffffff"
				android:textSize="30sp"
				android:textStyle="bold" />
		</RelativeLayout>
		<RelativeLayout
			android:layout_width="match_parent"
			android:layout_height="0dp"
			android:layout_weight="1"
			android:gravity="center"
			android:orientation="vertical" >
			<TextView
				android:id="@+id/selfas"
				android:layout_width="match_parent"
				android:layout_height="match_parent"
				android:layout_alignParentBottom="true"
				android:layout_alignParentRight="true"
				android:layout_alignParentTop="true"
				android:background="@drawable/menuselftest1"
				android:gravity="center"
				android:paddingLeft="40dp"
				android:text="@string/selfas"
				android:textColor="#ffffff"
				android:textSize="30sp"
				android:textStyle="bold" />
		</RelativeLayout>
		<RelativeLayout
			android:layout_width="match_parent"
			android:layout_height="0dp"
			android:layout_weight="1"
			android:gravity="center"
			android:orientation="vertical" >
			<TextView
				android:id="@+id/socialres"
				android:layout_width="match_parent"
				android:layout_height="match_parent"
				android:layout_alignParentBottom="true"
				android:layout_alignParentRight="true"
				android:layout_alignParentTop="true"
				android:background="@drawable/menusocres1"
				android:gravity="center"
				android:paddingLeft="40dp"
				android:text="@string/socialres"
				android:textColor="#ffffff"
				android:textSize="30sp"
				android:textStyle="bold" />
		</RelativeLayout>
		<RelativeLayout
			android:layout_width="match_parent"
			android:layout_height="0dp"
			android:layout_weight="1"
			android:gravity="center"
			android:orientation="vertical" >
			<TextView
				android:id="@+id/knowledge"
				android:layout_width="match_parent"
				android:layout_height="match_parent"
				android:layout_alignParentBottom="true"
				android:layout_alignParentRight="true"
				android:layout_alignParentTop="true"
				android:background="@drawable/menuknowledgr1"
				android:gravity="center"
				android:paddingLeft="40dp"
				android:text="@string/knowledge"
				android:textColor="#ffffff"
				android:textSize="30sp"
				android:textStyle="bold" />
		</RelativeLayout>
		<RelativeLayout
			android:layout_width="match_parent"
			android:layout_height="0dp"
			android:layout_weight="1"
			android:gravity="center"
			android:orientation="vertical" >
			<TextView
				android:id="@+id/baingame"
				android:layout_width="match_parent"
				android:layout_height="match_parent"
				android:layout_alignParentBottom="true"
				android:layout_alignParentRight="true"
				android:layout_alignParentTop="true"
				android:background="@drawable/menubraingame1"
				android:gravity="center"
				android:paddingLeft="40dp"
				android:text="@string/braingame"
				android:textColor="#ffffff"
				android:textSize="30sp"
				android:textStyle="bold" />
		</RelativeLayout>
		<RelativeLayout
			android:layout_width="match_parent"
			android:layout_height="0dp"
			android:layout_weight="1"
			android:gravity="center"
			android:orientation="vertical" >
			<TextView
				android:id="@+id/mylocation2"
				android:layout_width="150dp"
				android:layout_height="wrap_content"
				android:layout_alignParentBottom="true"
				android:layout_alignParentLeft="true"
				android:layout_alignParentRight="true"
				android:layout_alignParentTop="true"
				android:background="@drawable/menumylocation2_1"
				android:gravity="center"
				android:paddingLeft="40dp"
				android:text="@string/mylocation"
				android:textColor="#ffffff"
				android:textSize="30sp"
				android:textStyle="bold" />
		</RelativeLayout>
		<RelativeLayout
			android:layout_width="match_parent"
			android:layout_height="0dp"
			android:layout_weight="1"
			android:gravity="center"
			android:orientation="vertical" >
			<TextView
				android:id="@+id/callfmy"
				android:layout_width="150dp"
				android:layout_height="wrap_content"
				android:layout_alignParentBottom="true"
				android:layout_alignParentLeft="true"
				android:layout_alignParentRight="true"
				android:layout_alignParentTop="true"
				android:background="@drawable/menucallfmy1"
				android:gravity="center"
				android:paddingLeft="40dp"
				android:text="@string/callfmy"
				android:textColor="#ffffff"
				android:textSize="30sp"
				android:textStyle="bold" />
		</RelativeLayout>
		<RelativeLayout
			android:layout_width="match_parent"
			android:layout_height="0dp"
			android:layout_weight="1"
			android:gravity="center"
			android:orientation="vertical" >
			<TextView
				android:id="@+id/setting"
				android:layout_width="match_parent"
				android:layout_height="match_parent"
				android:layout_alignParentBottom="true"
				android:layout_alignParentRight="true"
				android:layout_alignParentTop="true"
				android:background="@drawable/menusetting1"
				android:gravity="center"
				android:paddingLeft="40dp"
				android:text="@string/setting"
				android:textColor="#ffffff"
				android:textSize="30sp"
				android:textStyle="bold" />
		</RelativeLayout>
		<RelativeLayout
			android:layout_width="match_parent"
			android:layout_height="0dp"
			android:layout_weight="1"
			android:gravity="center"
			android:orientation="vertical" >
		</RelativeLayout>
	</LinearLayout>
</LinearLayout>