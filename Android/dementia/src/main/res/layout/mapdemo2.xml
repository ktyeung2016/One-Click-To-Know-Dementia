<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:tools="http://schemas.android.com/tools"
	android:id="@+id/holepage"
	android:layout_width="match_parent"
	android:layout_height="match_parent" >
	<LinearLayout
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:orientation="vertical" >
		<RelativeLayout
			android:layout_width="wrap_content"
			android:layout_height="0dp"
			android:layout_weight="2" >
			<ImageView
				android:id="@+id/qweqeqe"
				android:layout_width="match_parent"
				android:layout_height="match_parent"
				android:layout_alignParentBottom="true"
				android:scaleType="fitXY"
				android:src="@drawable/actionbar" />
			<LinearLayout
				android:layout_width="match_parent"
				android:layout_height="match_parent"
				android:layout_alignParentRight="true" >
				<ImageButton
					android:id="@+id/back"
					android:layout_width="wrap_content"
					android:layout_height="match_parent"
					android:layout_weight="1"
					android:background="@null"
					android:src="@null"
                    android:contentDescription="@string/backButton"/>
				<TextView
					android:id="@+id/title"
					android:layout_width="wrap_content"
					android:layout_height="match_parent"
					android:layout_weight="3"
					android:gravity="center"
					android:text="@string/callfmy"
					android:textColor="#FFFFFF"
					android:textSize="?attr/font_large"
					android:textStyle="bold" />
				<ImageButton
					android:id="@+id/menu"
					android:layout_width="wrap_content"
					android:layout_height="match_parent"
					android:layout_weight="1"
					android:background="@null"
					android:src="@null"
                    android:contentDescription="@string/menuButton"/>
			</LinearLayout>
		</RelativeLayout>
		<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="0dp"
			android:layout_weight="20"
			android:orientation="vertical" >
			<LinearLayout
				android:layout_width="match_parent"
				android:layout_height="0dp"
				android:layout_weight="0.4"
				android:orientation="vertical" >
				<LinearLayout
					android:id="@+id/topbarlayout"
					android:layout_width="match_parent"
					android:layout_height="0dp"
					android:layout_weight="1"
					android:background="#ffffff"
					android:gravity="center"
					android:paddingLeft="10dp" >
					<TextView
						android:id="@+id/textView1"
						android:layout_width="0dp"
						android:layout_height="wrap_content"
						android:layout_weight="3"
						android:text="@string/howtoaddfmy"
						android:textSize="?attr/font_xsmall" />
					<ImageView
						android:id="@+id/laststep"
						android:layout_width="0dp"
						android:layout_height="wrap_content"
						android:layout_weight="0.4"
						android:scaleType="centerInside"
						android:src="@drawable/btn_back" />
					<TextView
						android:id="@+id/stagenumber"
						android:layout_width="0dp"
						android:layout_height="wrap_content"
						android:layout_weight="1.2"
						android:gravity="center"
						android:text="1/12"
						android:textSize="?attr/font_small" />
					<ImageView
						android:id="@+id/nextstep"
						android:layout_width="0dp"
						android:layout_height="wrap_content"
						android:layout_weight="0.4"
						android:scaleType="centerInside"
						android:src="@drawable/btn_next" />
				</LinearLayout>
			</LinearLayout>
			<LinearLayout
				android:layout_width="match_parent"
				android:layout_height="0dp"
				android:layout_weight="1"
				android:gravity="right|center"
				android:orientation="vertical"
				android:paddingRight="10dp" >
				<LinearLayout
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:gravity="right"
					android:orientation="vertical" >
					<Button
						android:id="@+id/newfmymem"
						android:layout_width="180dp"
						android:layout_height="50dp"
						android:background="@drawable/btnnewcallinfo"
						android:paddingLeft="25dp"
						android:text="@string/newafmymember"
						android:textColor="#ffffff" />
				</LinearLayout>
			</LinearLayout>
			<LinearLayout
				android:layout_width="match_parent"
				android:layout_height="0dp"
				android:layout_weight="4"
				android:orientation="vertical" >
				<ListView
					android:id="@+id/listView1"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:divider="@android:color/transparent"
					android:dividerHeight="10.0sp" >
				</ListView>
			</LinearLayout>
			<LinearLayout
				android:layout_width="match_parent"
				android:layout_height="0dp"
				android:layout_weight="1"
				android:background="@drawable/bgcalllowerpart"
				android:orientation="vertical" >
				<LinearLayout
					android:layout_width="match_parent"
					android:layout_height="0dp"
					android:layout_weight="2.2"
					android:orientation="vertical" >
					<TextView
						android:id="@+id/textView2"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:layout_weight="0.56"
						android:gravity="center"
						android:text="@string/hotline"
						android:textColor="#ffffff"
						android:textSize="?attr/font_large" />
				</LinearLayout>
				<LinearLayout
					android:layout_width="match_parent"
					android:layout_height="0dp"
					android:layout_weight="2.8"
					android:orientation="vertical" >
					<LinearLayout
						android:layout_width="match_parent"
						android:layout_height="match_parent" >
						<TextView
							android:id="@+id/textView3"
							android:layout_width="0dp"
							android:layout_height="match_parent"
							android:layout_weight="5"
							android:text="123456789"
							android:textSize="?attr/font_large" />
						<ImageButton
							android:id="@+id/imageButton1"
							android:layout_width="0dp"
							android:layout_height="wrap_content"
							android:layout_weight="1"
							android:background="@null"
							android:src="@drawable/btncalllowerpart" />
					</LinearLayout>
				</LinearLayout>
			</LinearLayout>
		</LinearLayout>
	</LinearLayout>
	<LinearLayout
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:orientation="vertical"
		android:visibility="gone" >
		<RelativeLayout
			android:layout_width="wrap_content"
			android:layout_height="0dp"
			android:layout_weight="2" >
		</RelativeLayout>
		<LinearLayout
			android:id="@+id/topbarlayout"
			android:layout_width="match_parent"
			android:layout_height="0dp"
			android:layout_weight="1"
			android:background="#ffffff"
			android:gravity="center"
			android:paddingLeft="10dp" >
			<TextView
				android:id="@+id/textView4"
				android:layout_width="0dp"
				android:layout_height="wrap_content"
				android:layout_weight="2"
				android:text="@string/mapfunctionstep"
				android:textSize="?attr/font_medium" />
			<ImageView
				android:id="@+id/imageView1"
				android:layout_width="0dp"
				android:layout_height="wrap_content"
				android:layout_weight="1"
				android:scaleType="centerInside"
				android:src="@drawable/btn_back" />
			<TextView
				android:id="@+id/textView5"
				android:layout_width="0dp"
				android:layout_height="wrap_content"
				android:layout_weight="1"
				android:gravity="center"
				android:text="2/12"
				android:textSize="?attr/font_small" />
			<ImageView
				android:id="@+id/imageView2"
				android:layout_width="0dp"
				android:layout_height="wrap_content"
				android:layout_weight="1"
				android:scaleType="centerInside"
				android:src="@drawable/btn_next" />
		</LinearLayout>
		<RelativeLayout
			android:id="@+id/demolayout"
			android:layout_width="match_parent"
			android:layout_height="0dp"
			android:layout_weight="18"
			android:background="@drawable/transparentbg"
			android:orientation="vertical" >
		</RelativeLayout>
	</LinearLayout>
    <RelativeLayout
        android:id="@+id/overlay"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:id="@+id/overlayImage"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />
    </RelativeLayout>
</RelativeLayout>