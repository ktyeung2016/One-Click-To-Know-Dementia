<resources>
	<!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
	-->
	<style name="AppBaseTheme" parent="Theme.AppCompat.Light">		<!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
		-->
	</style>
	<!-- Application theme. -->
	<style name="AppTheme" parent="AppBaseTheme"> <!-- All customizations that are NOT specific to a particular API-level can go here. -->
	</style>
	<style name="FontStyle"></style>
	<style name="FontStyle.Small">
		<item name="font_xsmall">14sp</item>
		<item name="font_small">16sp</item>
		<item name="font_medium">18sp</item>
		<item name="font_large">20sp</item>
		<item name="font_xlarge">22sp</item>
		<item name="font_xxlarge">24sp</item>
	</style>
	<style name="FontStyle.Medium">
		<item name="font_xsmall">18sp</item>
		<item name="font_small">22sp</item>
		<item name="font_medium">24sp</item>
		<item name="font_large">28sp</item>
		<item name="font_xlarge">30sp</item>
		<item name="font_xxlarge">38sp</item>
	</style>
	<style name="FontStyle.Large">
		<item name="font_small">26sp</item>
		<item name="font_medium">28sp</item>
		<item name="font_large">30sp</item>
		<item name="font_xlarge">32sp</item>
		<item name="font_xxlarge">40sp</item>
	</style>
	<style name="MyRadioButtonStyle" parent="@android:style/Widget.CompoundButton.RadioButton">
		<item name="android:button">@drawable/ratiobutton</item>
	</style>
</resources>